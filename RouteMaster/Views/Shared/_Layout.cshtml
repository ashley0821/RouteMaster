    <style>
        @@media (min-width: 1200px) {
            .layout-menu {
                position: fixed;
                top: 0;
                bottom: 0;
                left: 0;
                margin-right: 0 !important;
                margin-left: 0 !important;
                z-index: 1080;
                overscroll-behavior: none;
            }

            .layout-page {
                margin-left: 16.25rem;
            }

            .menu-inner {
                overflow-y: auto;
                overscroll-behavior: none;
            }

                .menu-inner::-webkit-scrollbar {
                    display: none;
                }
        }
    </style>
    @RenderSection("styles", required: false)

</head>
